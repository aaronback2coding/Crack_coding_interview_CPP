{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "helloworld",
            "type": "shell",
            "command": "g++ -std=c++11 -g helloworld.cpp -o helloworld",
            //            "command": "pwd",
            "cwd": "${workspaceFolder}/staticlinkjson/",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$gcc"
        }
    ]
}